{
  "name": "pwned",
  "version": "2.0.2-dev.0",
  "description": "A command-line tool for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": "Justin Hall <justin.r.hall@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "main": "bin/pwned.js",
  "bin": {
    "pwned": "./bin/pwned.js"
  },
  "directories": {
    "bin": "./bin",
    "lib": "./lib"
  },
  "scripts": {
    "clean": "rimraf ./lib ./coverage ./nyc_output",
    "compile": "babel src --out-dir lib --source-maps",
    "lint": "eslint bin src test",
    "prepublish": "npm run clean && npm run compile",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/pwned.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwned/issues"
  },
  "homepage": "https://github.com/wKovacs64/pwned",
  "engines": {
    "iojs": "*",
    "node": ">= 0.10"
  },
  "dependencies": {
    "cli-spinner": "^0.2.5",
    "commander": "^2.9.0",
    "es6-promise": "^3.3.1",
    "hibp": "^2.1.0",
    "prettyjson": "^1.1.3",
    "source-map-support": "^0.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-preset-es2015": "^6.14.0",
    "coveralls": "^2.11.14",
    "cross-env": "^2.0.1",
    "eslint": "^3.5.0",
    "eslint-config-semistandard": "^7.0.0",
    "eslint-config-standard": "^6.0.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "expect.js": "^0.3.1",
    "mocha": "^3.0.2",
    "mockery": "^1.7.0",
    "nyc": "^8.3.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6"
  },
  "preferGlobal": true
}
