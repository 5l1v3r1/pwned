{
  "name": "pwned",
  "version": "1.0.2",
  "description": "A command-line tool for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security"
  ],
  "author": "Justin Hall <justin.r.hall@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "main": "bin/pwned.js",
  "bin": {
    "pwned": "./bin/pwned.js"
  },
  "scripts": {
    "compile": "babel src --out-dir bin --source-maps",
    "prepublish": "npm run compile",
    "test": "semistandard --verbose | snazzy && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/pwned.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwned/issues"
  },
  "homepage": "https://github.com/wKovacs64/pwned",
  "eslintConfig": {
    "ecmaFeatures": {
      "blockBindings": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "engines": {
    "iojs": "*",
    "node": ">= 0.10"
  },
  "dependencies": {
    "cli-spinner": "^0.2.5",
    "commander": "^2.9.0",
    "es6-promise": "^3.1.2",
    "hibp": "^1.0.4",
    "prettyjson": "^1.1.3",
    "source-map-support": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "semistandard": "*",
    "snazzy": "^3.0.0"
  },
  "preferGlobal": true
}
