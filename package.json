{
  "name": "pwned",
  "version": "3.0.1-dev.0",
  "description": "A command-line tool for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": "Justin Hall <justin.r.hall@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "main": "bin/pwned.js",
  "bin": {
    "pwned": "./bin/pwned.js"
  },
  "directories": {
    "bin": "./bin",
    "lib": "./lib"
  },
  "scripts": {
    "clean": "rimraf ./lib ./coverage ./nyc_output",
    "compile": "babel src --out-dir lib --source-maps",
    "lint": "eslint bin src test",
    "prepublish": "npm run clean && npm run compile",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/pwned.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwned/issues"
  },
  "homepage": "https://wkovacs64.github.io/pwned",
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "cli-spinner": "^0.2.5",
    "commander": "^2.9.0",
    "hibp": "^3.0.0",
    "prettyjson": "^1.1.3",
    "source-map-support": "^0.4.6"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.15",
    "cross-env": "^3.1.3",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "mocha": "^3.1.2",
    "mockery": "^2.0.0",
    "nyc": "^9.0.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6"
  },
  "preferGlobal": true
}
